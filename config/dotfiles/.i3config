# My custom settings to i3!

# Monitor
exec xrandr --output VGA-1 --auto --left-of eDP-1

# Background
exec --no-startup-id xsetroot -solid "#000000"
exec xset s off

# Keyboards
exec setxkbmap -layout us -variant intl

bar { 
    position top 
    status_command i3blocks 
    tray_output primary 
}
